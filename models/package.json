{
  "name": "app",
  "version": "0.1.0",
  "engines": {
    "node": ">=12.16.1"
  },
  "scripts": {
    "env:local": "node tools/set-environment.js local",
    "env:dev": "node tools/set-environment.js development",
    "env:uat": "node tools/set-environment.js uat",
    "env:prod": "node tools/set-environment.js production",
    "detox:android:debug:build": "npm run env:local && detox build -c android.emu.debug",
    "detox:android:debug:test": "npm run env:local && detox test -c android.emu.debug",
    "detox:android:release:build": "npm run env:local && detox build -c android.emu.release",
    "detox:android:release:test": "npm run env:local && detox test -c android.emu.release",
    "detox:ios:debug:build": "npm run env:local && detox build -c ios.sim.debug",
    "detox:ios:debug:test": "npm run env:local && detox test -c ios.sim.debug",
    "detox:ios:release:build": "npm run env:local && detox build -c ios.sim.release",
    "detox:ios:release:test": "npm run env:local && detox test -c ios.sim.release",
    "start": "react-native start",
    "start:nointeractive": "react-native start --no-interactive",
    "test": "npm run env:dev && jest",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "_android": "react-native run-android",
    "android": "npm run env:dev && npm run _android",
    "android:local": "npm run env:local && npm run _android",
    "android:uat": "npm run env:uat && npm run _android",
    "android:prod": "npm run env:prod && npm run _android",
    "_ios": "react-native run-ios",
    "ios": "npm run env:dev && npm run _ios",
    "ios:local": "npm run env:local && npm run _ios",
    "ios:uat": "npm run env:uat && npm run _ios",
    "ios:prod": "npm run env:prod && npm run _ios",
    "ios:pod:install": "cd ./ios && pod install && cd ..",
    "web": "expo start --web",
    "postinstall": "jetify",
    "eslint": "eslint \"**/*.{js,jsx}\"",
    "rm:gen": "rm -rf package-lock.json node_modules ios/build ios/Pods ios/Podfile.lock android/build android/app/build",
    "bundle:ios": "node --expose-gc --max_old_space_size=8192 ./node_modules/react-native/local-cli/cli.js bundle --entry-file index.js --platform ios --dev false --bundle-output ./ios/main.jsbundle --assets-dest ios",
    "bundle:android": "node --expose-gc --max_old_space_size=8192 ./node_modules/react-native/local-cli/cli.js bundle --assets-dest ./android/app/src/main/res/ --entry-file ./index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --platform android --dev false",
    "fastlane:android:build": "fastlane android build_release",
    "fastlane:android:githubrelease": "fastlane android github_release",
    "fastlane:android:firebase_distribution": "fastlane android firebase_distribute"
  },
